{
	"name": "messaging-service",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "nodemon ./bin/www",
		"lint": "lint-staged",
		"lint-all": "pretty-quick && eslint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"lint-staged": {
		"*.js": [
			"pretty-quick --staged",
			"eslint --fix"
		],
		"*.+(json|css|md)": [
			"pretty-quick --staged"
		]
	},
	"author": "Ozan Ta≈üar <tasarozan@gmail.com>",
	"dependencies": {
		"connect-mongo": "^4.6.0",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"express-session": "^1.17.2",
		"http-errors": "~1.6.3",
		"mongoose": "^6.0.8",
		"mongoose-autopopulate": "^0.16.0",
		"morgan": "~1.9.1",
		"passport": "^0.5.0",
		"passport-local": "^1.0.0",
		"passport-local-mongoose": "^6.1.0",
		"pug": "2.0.0-beta11",
		"socket.io": "^4.2.0"
	},
	"devDependencies": {
		"connect-livereload": "^0.6.1",
		"eslint": "^7.2.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.5",
		"eslint-plugin-react-hooks": "^4.0.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"livereload": "^0.9.1",
		"nodemon": "^2.0.12",
		"prettier": "2.0.5",
		"pretty-quick": "^2.0.1"
	}
}
